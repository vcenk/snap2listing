# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# FAL.ai API Key (for image/video generation)
FAL_KEY=your-fal-api-key

# OpenAI API Key (for text generation)
OPENAI_API_KEY=your-openai-api-key

# Cloudflare R2 Storage Configuration
CLOUDFLARE_ACCOUNT_ID=your-account-id
R2_ACCESS_KEY_ID=your-r2-access-key
R2_SECRET_ACCESS_KEY=your-r2-secret-key
R2_BUCKET_NAME=your-bucket-name
NEXT_PUBLIC_R2_PUBLIC_URL=https://your-bucket.r2.dev

# Etsy OAuth Configuration
ETSY_CLIENT_ID=your-etsy-client-id
ETSY_CLIENT_SECRET=your-etsy-client-secret
ETSY_REDIRECT_URI=http://localhost:3000/api/etsy/callback

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_...
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Stripe Price IDs (create these in your Stripe Dashboard)
STRIPE_PRICE_STARTER_MONTHLY=price_...
STRIPE_PRICE_STARTER_YEARLY=price_...
STRIPE_PRICE_PRO_MONTHLY=price_...
STRIPE_PRICE_PRO_YEARLY=price_...
STRIPE_PRICE_ENTERPRISE_MONTHLY=price_...
STRIPE_PRICE_ENTERPRISE_YEARLY=price_...

# Stripe Add-on Price IDs
STRIPE_PRICE_ADDON_IMAGES=price_...  # 20 images for $10
STRIPE_PRICE_ADDON_VIDEOS=price_...  # 2 videos for $10

# App URL (for redirects)
NEXT_PUBLIC_URL=http://localhost:3000
